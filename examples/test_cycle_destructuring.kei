declare("=== Keikaku Advanced Loop Destructuring Test ===")

# 1. Unpacking in cycles
pairs := [[1, "apple"], [2, "banana"], [3, "cherry"]]

declare("Unpacking inventory:")
cycle through pairs as [id, name]:
    declare("- ID:", id, "Item:", name)

# 2. Nested unpacking
complex := [[1, [10, 20]], [2, [30, 40]]]
cycle through complex as [id, [low, high]]:
    declare("Node:", id, "Range:", low, "to", high)

# 3. Pattern from-to (identifier style)
declare("Counting:")
cycle from 1 to 4 as i:
    declare(i)

declare("=== Test Complete! ===")
